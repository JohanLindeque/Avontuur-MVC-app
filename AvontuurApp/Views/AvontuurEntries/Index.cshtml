@model List<AvontuurEntry>

<div class="table-responsive">
    .<table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Title</th>
                <th>Content</th>
                <th>Location</th>
                <th>Date</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var entry in Model)
            {
                <tr>
                    <td>@entry.Title</td>
                    <td>@entry.Content</td>
                    <td>@entry.Location</td>
                    <td>@entry.CreatedAt</td>

                </tr>
            }
        </tbody>
    </table>


</div>